||| Generators for typical unicode ranges and categories
module Hedgehog.Gen.Unicode

import Data.Vect
import Hedgehog.Internal.Gen
import Hedgehog.Internal.Range
import Hedgehog.Internal.Util

%default total

export %inline
charHex : Bits32 -> Gen Char
charHex l = pure (cast l)

export %inline
rangeHex : Bits32 -> Bits32 -> Gen Char
rangeHex l u = map cast $ bits32 (linear l u)

||| Currently unassigned unicode code points (General Category: Cn).
export
unassigned : Gen Char

||| Unicode codepoints of the `Uppercase_Letter` (Lu)
||| general category
export
uppercaseLetter : Gen Char

||| Unicode codepoints of the `Lowercase_Letter` (Ll)
||| general category
export
lowercaseLetter : Gen Char

||| Unicode codepoints of the `Titlecase_Letter` (Ll)
||| general category
export
titlecaseLetter : Gen Char

||| Unicode codepoints of the `Modifier_Letter` (Lm)
||| general category
export
modifierLetter : Gen Char

||| Unicode codepoints of the `Other_Letter` (Lo)
||| general category
export
otherLetter : Gen Char

||| Unicode codepoints of the `Letter` (L)
||| general category
export
letter : Gen Char
letter =
  choice
    [ uppercaseLetter
    , lowercaseLetter
    , titlecaseLetter
    , modifierLetter
    , otherLetter
    ]

||| Unicode codepoints of the `Cased_Letter` (LC)
||| general category
export
casedLetter : Gen Char
casedLetter =
  choice
    [ uppercaseLetter
    , lowercaseLetter
    , titlecaseLetter
    ]

||| Unicode codepoints of the `Nonspacing_Mark` (Mn)
||| general category
export
nonspacingMark : Gen Char

||| Unicode codepoints of the `Enclosing_Mark` (Me)
||| general category
export
enclosingMark : Gen Char

||| Unicode codepoints of the `Spacing_Mark` (Mc)
||| general category
export
spacingMark : Gen Char

||| Unicode codepoints of the `Mark` (M)
||| general category
export
mark : Gen Char
mark = choice [nonspacingMark, enclosingMark, spacingMark]

||| Unicode codepoints of the `Decimal_Number` (Nd)
||| general category
export
decimalNumber : Gen Char

||| Unicode codepoints of the `Letter_Number` (Nl)
||| general category
export
letterNumber : Gen Char

||| Unicode codepoints of the `Other_Number` (No)
||| general category
export
otherNumber : Gen Char

||| Unicode codepoints of the `Number` (N)
||| general category
export
number : Gen Char
number = choice [decimalNumber, letterNumber, otherNumber]

||| Unicode codepoints of the `Space_Separator` (Zs)
||| general category
export
spaceSeparator : Gen Char

||| Unicode codepoints of the `Line_Separator` (Zl)
||| general category
export
lineSeparator : Gen Char

||| Unicode codepoints of the `Paragraph_Separator` (Zp)
||| general category
export
paragraphSeparator : Gen Char

||| Unicode codepoints of the `Separator` (Z)
||| general category
export
separator : Gen Char
separator = choice [spaceSeparator, lineSeparator, paragraphSeparator]

||| Unicode codepoints of the `Control` (Cc)
||| general category
export
control : Gen Char

||| Unicode codepoints of the `Format` (Cf)
||| general category
export
format : Gen Char

||| Unicode codepoints of the `Private_Use` (Co)
||| general category
export
privateUse : Gen Char

||| Unicode codepoints of the `Dash_Punctuation` (Pd)
||| general category
export
dashPunctuation : Gen Char

||| Unicode codepoints of the `Open_Punctuation` (Ps)
||| general category
export
openPunctuation : Gen Char

||| Unicode codepoints of the `Close_Punctuation` (Pe)
||| general category
export
closePunctuation : Gen Char

||| Unicode codepoints of the `Connector_Punctuation` (Pc)
||| general category
export
connectorPunctuation : Gen Char

||| Unicode codepoints of the `Other_Punctuation` (Po)
||| general category
export
otherPunctuation : Gen Char

||| Unicode codepoints of the `Initial_Punctuation` (Pi)
||| general category
export
initialPunctuation : Gen Char

||| Unicode codepoints of the `Final_Punctuation` (Pf)
||| general category
export
finalPunctuation : Gen Char

||| Unicode codepoints of the `Punctuation` (P)
||| general category
export
punctuation : Gen Char
punctuation =
  choice
    [ dashPunctuation
    , openPunctuation
    , closePunctuation
    , connectorPunctuation
    , otherPunctuation
    , initialPunctuation
    , finalPunctuation
    ]

||| Unicode codepoints of the `Math_Symbol` (Sm)
||| general category
export
mathSymbol : Gen Char

||| Unicode codepoints of the `Currency_Symbol` (Sc)
||| general category
export
currencySymbol : Gen Char

||| Unicode codepoints of the `Modifier_Symbol` (Sk)
||| general category
export
modifierSymbol : Gen Char

||| Unicode codepoints of the `Other_Symbol` (So)
||| general category
export
otherSymbol : Gen Char

||| Unicode codepoints of the `Symbol` (S)
||| general category
export
symbol : Gen Char
symbol =
  choice
    [ mathSymbol
    , currencySymbol
    , modifierSymbol
    , otherSymbol
    ]

||| Unicode codepoints of the `Other` (C)
||| general category
export
other : Gen Char
other =
  choice
    [ control
    , format
    , privateUse
    , unassigned
    ]

